<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_margin="10sp"
    android:padding="30sp"
    android:background="@drawable/my_log_fragment_list_layout"
    android:elevation="10sp">

    <TextView
        android:id="@+id/my_log_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Title"
        android:fontFamily = "@font/imbplexmono_light"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:layout_width="25sp"
        android:layout_height="25sp"
        android:textColor="@color/right_arrow"
        android:text=">"
        android:textAlignment="center"
        app:layout_constraintLeft_toRightOf="@id/my_log_title"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/my_log_content1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10sp"
        android:text="content1"
        android:fontFamily = "@font/imbplexmono_light"
        app:layout_constraintTop_toBottomOf="@id/my_log_title"
        app:layout_constraintLeft_toLeftOf="parent"
        android:background="@drawable/my_log_fragment_textview_underline"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/my_log_content2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="content2"
        android:layout_marginTop="5sp"
        android:fontFamily = "@font/imbplexmono_light"
        app:layout_constraintTop_toBottomOf="@id/my_log_content1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:background="@drawable/my_log_fragment_textview_underline"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/my_log_content3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="content3"
        android:layout_marginTop="5sp"
        android:fontFamily = "@font/imbplexmono_light"
        app:layout_constraintTop_toBottomOf="@id/my_log_content2"
        app:layout_constraintLeft_toLeftOf="parent"
        android:background="@drawable/my_log_fragment_textview_underline"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/my_log_content4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="content4"
        android:layout_marginTop="5sp"
        android:fontFamily = "@font/imbplexmono_light"
        app:layout_constraintTop_toBottomOf="@id/my_log_content3"
        app:layout_constraintLeft_toLeftOf="parent"
        android:background="@drawable/my_log_fragment_textview_underline"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/my_log_content5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="content5"
        android:layout_marginTop="5sp"
        android:fontFamily = "@font/imbplexmono_light"
        app:layout_constraintTop_toBottomOf="@id/my_log_content4"
        app:layout_constraintLeft_toLeftOf="parent"
        android:background="@drawable/my_log_fragment_textview_underline"
        tools:ignore="MissingConstraints" />



</androidx.constraintlayout.widget.ConstraintLayout>